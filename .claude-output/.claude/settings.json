{
  "permissions": {
    "allow": [
      "Read(**/*.ts)",
      "Read(**/*.tsx)",
      "Read(**/*.js)",
      "Read(**/*.jsx)",
      "Read(**/*.py)",
      "Read(package.json)",
      "Read(package-lock.json)",
      "Read(yarn.lock)",
      "Write(package.json)",
      "Write(package-lock.json)",
      "Bash(npm *)",
      "Bash(yarn *)",
      "Read(**/*.yaml)",
      "Read(**/*.json)",
      "Write(src/api/**)",
      "Write(src/routes/**)",
      "Write(openapi.yaml)"
    ],
    "deny": [
      "Read(.env)",
      "Read(.env.*)",
      "Read(**/secrets/**)",
      "Write(**)",
      "Bash(rm -rf *)",
      "Bash(* --force)",
      "Write(src/config/production.*)",
      "Bash(rm *)"
    ]
  },
  "env": {},
  "enabledPlugins": {},
  "hooks": {}
}