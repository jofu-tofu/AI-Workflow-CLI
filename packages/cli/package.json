{
  "name": "aiwcli",
  "description": "AI Workflow CLI - Command-line interface for AI-powered workflows",
  "version": "0.0.0",
  "author": "jofu-tofu",
  "bin": {
    "aiw": "./bin/run.js"
  },
  "bugs": {
    "url": "https://github.com/jofu-tofu/AI-Workflow-CLI/issues"
  },
  "dependencies": {
    "@inquirer/prompts": "^8.2.0",
    "@oclif/core": "^4",
    "@oclif/plugin-autocomplete": "^3.2.39",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "chalk": "^5.6.2",
    "clipboardy": "^5.0.2",
    "ora": "^9.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "@types/sinon": "^21.0.0",
    "c8": "^10.1.3",
    "chai": "^4",
    "chokidar": "^5.0.0",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "mocha": "^10",
    "npm-run-all2": "^8.0.4",
    "oclif": "^4",
    "prettier": "^3.7.4",
    "shx": "^0.3.3",
    "sinon": "^21.0.1",
    "stdout-stderr": "^0.1.13",
    "strip-ansi": "^7.1.2",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/jofu-tofu/AI-Workflow-CLI#readme",
  "keywords": [
    "oclif",
    "cli",
    "aiwcli",
    "ai-workflow",
    "claude-code",
    "ai-cli"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "aiw",
    "dirname": "aiw",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "repositoryPrefix": "https://github.com/jofu-tofu/AI-Workflow-CLI/blob/main/<%- commandPath %>",
    "topicSeparator": " "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jofu-tofu/AI-Workflow-CLI.git"
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b && shx cp -r src/templates dist/",
    "build:watch": "tsc -b --watch",
    "check": "npm run lint && npm run build",
    "clean": "shx rm -rf dist",
    "dev": "npm run build && node --loader ts-node/esm bin/dev.js",
    "dev:watch": "run-p build:watch templates:watch",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme",
    "templates:watch": "node --loader ts-node/esm src/lib/watch-templates.ts",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "test:watch": "mocha --watch \"test/**/*.test.ts\"",
    "test:coverage": "c8 npm test",
    "version": "oclif readme && git add README.md",
    "watch": "run-p dev:watch test:watch"
  },
  "types": "dist/index.d.ts"
}
